<div class="row">
    <div class="col-md-12">
        <Br />
        <h2>List of All Student</h2>
        <Br />
        <table class="table table-dark table-bordered">
            <tr>
                <th>S.No.</th>
                <th>Name</th>

                <th>View</th>
                <th>Edit</th>
                <th>Delete</th>
            </tr>
            <% data.forEach((x, n)=>{ %>
                <tr>
                    <td>
                        <%= n+1 %>
                    </td>
                    <td>
                        <%= x.name %>
                    </td>
                    <!-- <td><%= x.age %></td> -->
                    <!-- <td><%= x.fee %></td> -->
                    <td><a href="#" class="btn btn-info btn-sm">View</a></td>
                    <td><a href="#" class="btn btn-warning btn-sm">Edit</a></td>
                    <td><a href="#" class="btn btn-danger btn-sm">Delete</a></td>
                </tr>
                <% }) %>

        </table>
    </div>
</div>