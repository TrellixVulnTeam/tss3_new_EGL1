<html>

<head>
    <style>
        body {
            background-color: #ACE124;
        }
    </style>
</head>

<body>
    <h2>All Student</h2>
    <table border="1" align="center" width="400">
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>City</th>
        </tr>

        <% stu.forEach((x)=>{ %>
            <tr>
                <td>
                    <%= x.name %>
                </td>
                <td>
                    <%= x.age %>
                </td>
                <td>
                    <%= x.city %>
                </td>
            </tr>

            <% }) %>


    </table>
</body>

</html>